node {
   state = env.PR_STATE
   targetJob = params.TARGET_JOB
   branch = null
   if (state == "OPEN") {
       branch = env.PR_SOURCE_BRANCH;
   }
   
   if (branch != null) {
       build job: targetJob, wait: false, parameters: [
              [$class: 'StringParameterValue', name: 'BRANCH_NAME', value: branch],
              [$class: 'BooleanParameterValue', name: 'UPSTREAM_BUILD', value: true]
          ]
   }
}